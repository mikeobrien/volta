<bindings>
  <element name="Form">
    <start>this.FormFor&lt;@model&gt;().Attr("class", "validate")</start>
    <end>this.EndForm()</end>
  </element>
  <element name="LabelFor">this.LabelFor(m =&gt; m.@property)</element>
  <element name="InputFor">this.InputFor(m =&gt; m.@property)</element>
</bindings>
