<viewdata model="Volta.Web.Controllers.Login.LoginViewModel" />
<content:title>Login</content:title>
<content:header></content:header>

<h1>Login</h1>

<Form model="Volta.Web.Controllers.Login.LoginViewModel">
  <div>
    <LabelFor property="Username" />
    <InputFor property="Username" />
  </div>
  <div>
    <LabelFor property="Password" />
    <InputFor property="Password" />
  </div>
  <div>
    <button type="submit">
      <span>Login</span>
    </button>
  </div>
</Form>